    <!-- This script handles the tabbed pane functionality -->
    <script type="text/javascript">

        jQuery(document).ready(function(){

            prepareList();

            $("#addDeviceForm").dialog({
                autoOpen:false,
                height: 500,
                width: 800,
                modal: true,
                buttons:{
                    'Confirm': function() {
                        var deviceType=$("#deviceTypeSelect option:selected").text();
                        var purpose=$("#purposeArea").val();
                        var requirements=$("#requirementArea").val();


                        $.get("test.jag", {purpose: purpose,requirements:requirements ,deviceType:deviceType, action: "printData"},
                                function(data){
                                    $( ".myRequests" ).append( data );
                                });

                        alert(deviceType+" "+purpose+" "+requirements);
//                        window.location.href = "test.jag";
                        $(this).dialog('close');

                    }
                }
            });

            $("#addDeviceButton").click(function(event)
            {
                //Stop any default actions on the button.
                event.preventDefault();
                //Open your dialog.
                $("#addDeviceForm").dialog("open");

            });

            function prepareList() {
                $('#expList').find('li:has(ul)')
                        .click( function(event) {
                            if (this == event.target) {
                                $(this).toggleClass('expanded');
                                $(this).children('ul').toggle('medium');
                            }
                            return false;
                        })
                        .addClass('collapsed')
                        .children('ul').hide();
            };







