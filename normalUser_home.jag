      <!-- This script handles the tabbed pane functionality -->
      <script>
         jQuery(document).ready(function(){
         
             <!--*******************-->

         prepareList();
         
         
         function prepareList() {
         $('#expList').find('li:has(ul)')
             .click( function(event) {
                 if (this == event.target) {
                     $(this).toggleClass('expanded');
                     $(this).children('ul').toggle('medium');
                 }
                 return false;
             })
             .addClass('collapsed')
             .children('ul').hide();
         };
         
         
         $("#requestDeviceForm").dialog({
         autoOpen:false,
         height: 500,
         width: 800,
         modal: true,
         buttons:{
         'Confirm': function() {
             var deviceType=$("#deviceTypeSelect option:selected").text();
             var purpose=$("#purposeArea").val();
             var requirements=$("#requirementArea").val();
         
             //(deviceType+" "+purpose+" "+requirements);
         //                        window.location.href = "test.jag";
             Hwdrepo.requestDevice(deviceType,purpose,requirements);
             $(this).dialog('close');

             Hwdrepo.viewRequests();
         
         }
         }
         });

          $("#requestDeviceForm").dialog('option', 'title', 'Request a Device');
         
         $("#addRequestButton").click(function(event)
         {
         //Stop any default actions on the button.
         event.preventDefault();
         //Open your dialog.
         $("#requestDeviceForm").dialog("open");
         
         });

         $('#allRequests').html( '<table cellpadding="0" cellspacing="0" border="0" class="display" id="example"></table>' );
         /***********************************/







          /***********************************/
         
         Hwdrepo.viewRequests();

         
         Hwdrepo.loadDevices();

});
         
