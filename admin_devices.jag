            <!-- This script handles the tabbed pane functionality -->
            <script type="text/javascript">
                $(document).ready(function(){

                    var basicFields=new Array();

                    basicFields=[$("#serial"),$("#model"),$('#make'),$("purchaseDate"),$('#supplier'),$('#remarks'),$("#deviceType")];
                    var dropdownSelect="";
                
                    getAllDevices();
                
                    $("#addDevicePopup").dialog({
                        autoOpen:false,
                        height: 600,
                        width:850,
                        modal: true,
                        buttons:{
                            'Confirm': function() {
                            var deviceType=$('#deviceType').val();

                            alert(deviceType);

                                for(var i=0;i<basicFields.length;i++){
                                    if(basicFields[i].val()==""){
                                            //alert(deviceType);
                                            basicFields[i].removeClass("inputField");
                                            basicFields[i].addClass("errorInputField");
                                    }
                                    else{
                                            basicFields[i].removeClass("errorInputField");
                                            basicFields[i].addClass("inputField");
                                    }
                                }

                                switch (deviceType){

                                    case '0':
                                    break;

                                    case '1':{
                                        var padFields=new Array();

                                        padFields=[$("#padScreen"),$("#padTouch"),$("#padCam"),$("#padVga")];
                                        for(var i=0;i<padFields.length;i++){

                                            if(padFields[i].val()==""){

                                                padFields[i].removeClass("inputField");
                                                padFields[i].addClass("errorInputField");
                                            }
                                            else{
                                                    padFields[i].removeClass("errorInputField");
                                                    padFields[i].addClass("inputField");
                                            }
                                        }

                                        break;
                                    }



                                    case '2':{

                                        var desktopsFields=new Array();

                                        desktopsFields=[$("#dtProcessor"),$("#dtMemory"),$("#dtVga"),$("#dtHdd")];
                                        for(var i=0;i<desktopsFields.length;i++){
                                            if(desktopsFields[i].val()==""){

                                                desktopsFields[i].removeClass("inputField");
                                                desktopsFields[i].addClass("errorInputField");
                                            }
                                            else{
                                                    desktopsFields[i].removeClass("errorInputField");
                                                    desktopsFields[i].addClass("inputField");
                                            }
                                        }

                                        break;
                                    }


                                    case '3':
                                        var laptopsFields=new Array();

                                        laptopsFields=[$("#lapScreen"),$("#lapProcessor"),$("#lapMemory"),$("#lapVga"),$("#lapHdd")];
                                        for(var i=0;i<laptopsFields.length;i++){
                                            if(laptopsFields[i].val()==""){

                                                laptopsFields[i].removeClass("inputField");
                                                laptopsFields[i].addClass("errorInputField");
                                            }
                                            else{
                                                    laptopsFields[i].removeClass("errorInputField");
                                                    laptopsFields[i].addClass("inputField");
                                            }
                                        }
                                    break;
                                }
                            }
                        }
                    });
                
                
                    $("#addNewDevice").click(function(){
                        event.preventDefault();
                        $("#addDevicePopup").dialog("open");
                    });
                
                    $('#deviceType').change(function(){
                
                        var option = document.getElementById("deviceType");
                        var selectedVal = option.options[option.selectedIndex].value;
                
                        if(selectedVal=="1"){
                            document.getElementById('pads').style.display="";
                            if(dropdownSelect!=""){
                                document.getElementById(dropdownSelect).style.display="none";
                            }
                            dropdownSelect="pads";
                        }
                        else if(selectedVal=='2'){
                            document.getElementById('desktops').style.display="";
                            if(dropdownSelect!=""){
                                document.getElementById(dropdownSelect).style.display="none";
                            }
                            dropdownSelect="desktops";
                        }
                        else if(selectedVal=='3'){
                            document.getElementById('laptops').style.display="";
                            if(dropdownSelect!=""){
                                document.getElementById(dropdownSelect).style.display="none";
                            }
                            dropdownSelect="laptops";
                        }
                        else if(selectedVal=='4'){
                            document.getElementById('server').style.display="";
                            if(dropdownSelect!=""){
                                document.getElementById(dropdownSelect).style.display="none";
                            }
                            dropdownSelect="server";
                        }
                        else if(selectedVal=='5'){
                            document.getElementById('peripheral').style.display="";
                            if(dropdownSelect!=""){
                                document.getElementById(dropdownSelect).style.display="none";
                            }
                            dropdownSelect="peipheral";
                        }
                
                    });
                
                    $(function() {
                        $( "#purchaseDate" ).datepicker();
                    });


                    $("#searchDevices").click(function(){
                        var userName=document.getElementById("userName");
                        var serialNum=document.getElementById('serialNum');
                        var errSpan=document.getElementById("searchErr");

                        if(userName.value=="" && serialNum.value==""){
                            errSpan.classList.remove(errSpan.className);
                            errSpan.classList.add("error");
                            errSpan.textContent="Please insert search Text"
                        }
                        else{

                            var tableArea=document.getElementById('deviceDetailsDiv');
                            var devicesTbl=document.getElementById('devices');
                            var tbody=document.createElement('tbody');

                            tableArea.setAttribute("class","datagrid");
                            devicesTbl.style.width="800px";


                            errSpan.classList.remove(errSpan.className);
                            errSpan.classList.add("hidden");

                            while ( devicesTbl.rows.length > 1 )
                            {
                                devicesTbl.deleteRow(1);
                            }


                            $.post("admin_devices_operations.jag",{operation:"getSearchedDevices"},function(data,status){

                                var objArray=JSON.parse(data);




                                var i=0;

                                for(var i=0;i<objArray.length;i++){

                                    var newRow=document.createElement('tr');

                                    if(i%2==0){
                                        newRow.setAttribute("class","alt");
                                    }

                                    var object=objArray[i];

                                    for(var property in object){

                                        var td=document.createElement('td');

                                        td.appendChild(document.createTextNode(object[property]));

                                        newRow.appendChild(td);
                                    }
                                    var link=document.createElement('a');
                                    link.setAttribute("href","admin_device_details.jag");
                                    link.innerHTML="more";
                                    var  linkTD=document.createElement("td");
                                    linkTD.appendChild(link);
                                    newRow.appendChild(linkTD);

                                    tbody.appendChild(newRow);
                                }

                                devicesTbl.appendChild(tbody);

                            });
                        }
                    });
                });
                
                
                
                
                
                function getAllDevices(){
                
                    var tableArea=document.getElementById('deviceDetailsDiv');
                    var devicesTbl=document.getElementById('devices');
                    var tbody=document.createElement('tbody');
                
                    tableArea.setAttribute("class","datagrid");
                    devicesTbl.style.width="800px";
                
                    $.post("admin_devices_operations.jag",{operation:"getAllDevices"},function(data,status){
                
                        var objArray=JSON.parse(data);
                
                        var i=0;
                
                
                
                        for(var i=0;i<objArray.length;i++){
                
                            var newRow=document.createElement('tr');
                
                            if(i%2==0){
                                newRow.setAttribute("class","alt");
                            }
                
                            var object=objArray[i];
                
                            for(var property in object){
                
                                var td=document.createElement('td');
                
                                td.appendChild(document.createTextNode(object[property]));
                
                                newRow.appendChild(td);
                            }
                            var link=document.createElement('a');
                            link.setAttribute("href","admin_device_details.jag");
                            link.innerHTML="more";
                            var  linkTD=document.createElement("td");
                            linkTD.appendChild(link);
                            newRow.appendChild(linkTD);
                
                            tbody.appendChild(newRow);
                        }
                
                        devicesTbl.appendChild(tbody);
                
                    });
                }
                
                
                
            </script>
