                        $( "#purchaseDate" ).datepicker({ dateFormat: 'yy-mm-dd+05:30'});
                        $("#warrantyEndDate").datepicker({ dateFormat: 'yy-mm-dd+05:30'});
                        $( "#accPurchase" ).datepicker({ dateFormat: 'yy-mm-dd+05:30'});
                        $("#accWarEnd").datepicker({ dateFormat: 'yy-mm-dd+05:30'});
                    });


                    $("#searchDevices").click(function(){
                        var userName=document.getElementById("userName");
                        var serialNum=document.getElementById('serialNum');
                        var deviceModel=document.getElementById("deviceModel");
                        var deviceTypeSelect=document.getElementById("deviceTypeSelect");
                        var radioGroup=document.getElementsByName("searchType");
                        var errSpan=document.getElementById("searchErr");
                        var queryType;

                        for(var a=0;a<radioGroup.length;a++){
                            if(radioGroup[a].checked){
                                if(radioGroup[a].value=="0"){
                                    if(serialNum.value==""){
                                        errSpan.classList.remove(errSpan.className);
                                        errSpan.classList.add("error");
                                        errSpan.textContent="Please insert search Text";
                                    }
                                    else{
                                        queryType=0;
                                        getSearchedDevices(queryType,serialNum.value);
                                    }
                                }
                                else if(radioGroup[a].value=="1"){
                                    if(userName.value==""){
                                        errSpan.classList.remove(errSpan.className);
                                        errSpan.classList.add("error");
                                        errSpan.textContent="Please insert search Text";
                                    }
                                    else{
                                        queryType=1;
                                        getSearchedDevices(queryType,userName.value);
                                    }
                                }
                                else if(radioGroup[a].value=="2"){
                                    if(deviceModel.value==""){
                                        errSpan.classList.remove(errSpan.className);
                                        errSpan.classList.add("error");
                                        errSpan.textContent="Please insert search Text";
                                    }
                                    else{
                                        queryType=2;
                                        getSearchedDevices(queryType,deviceModel.value);
                                    }
                                }
                                else if(radioGroup[a].value=="3"){
                                    if(deviceTypeSelect.value=="0"){
                                        errSpan.classList.remove(errSpan.className);
                                        errSpan.classList.add("error");
                                        errSpan.textContent="Please insert search Text";
                                    }
                                    else{
                                        queryType=3;
                                        getSearchedDevices(queryType,deviceTypeSelect.options[deviceTypeSelect.selectedIndex].text);
                                    }
                                }
                            }
                            else{
                                    errSpan.classList.remove(errSpan.className);
                                    errSpan.classList.add("error");
                                    errSpan.textContent="Please insert search Text";
                                }
                        }
                    });
                });

                
            </script>
