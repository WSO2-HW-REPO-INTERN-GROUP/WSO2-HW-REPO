                                                    desktopsFields[i].addClass("inputField");
                                                    //itemsArr[desktopsFields[i].attr('id')]=desktopsFields[i].val();
                                                    inputData[desktopsFields[i].attr('id')]=desktopsFields[i].val();
                                            }
                                        }
                                        //inputJsonObj["deskTop"]=itemsArr;
                                        inputData['remarks']=remarks;
                                        //queryType="AddDesktop";

                                        inputJsonObj["AddServerRoomDesktop"]=inputData;
                                        inputJsonObj['names']=namesArray;
                                        break;
                                    }


                                    case '6':{
                                        var otherFields=new Array();
                                        var namesArray=['sn','make','model','rating','date','supplier','type','remarks'];
                                        //var itemsArr={};
                                        inputJsonObj["type"]="AddOtherDevice";
                                        otherFields=[$("#type")];


                                        for(var i=0;i<otherFields.length;i++){
                                            if(otherFields[i].val()==""){
                                                otherFields[i].removeClass("inputField");
