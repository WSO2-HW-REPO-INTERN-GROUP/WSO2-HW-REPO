                                            }
                                        }

                                        //inputJsonObj["laptop"]=itemsArr;
                                        inputData['remarks']=remarks;

                                        //queryType="AddLaptop";

                                        inputJsonObj["laptopData"]=inputData;
                                        inputJsonObj['names']=namesArray;

                                        break;
                                    }

                                    case '5':{
                                        var monitorFields=new Array();
                                        var namesArray=['sn','make','model','rating','date','supplier','screen','remarks'];
                                        //var itemsArr={};
                                        inputJsonObj["type"]="AddLcdMonitor";
                                        monitorFields=[$("#screen")];


                                        for(var i=0;i<monitorFields.length;i++){
                                            if(monitorFields[i].val()==""){
                                                monitorFields[i].removeClass("inputField");
                                                monitorFields[i].addClass("errorInputField");
                                                submitError=true;
                                            }
                                            else{
