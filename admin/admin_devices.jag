                                    if(basicFields[i].val()==""){
                                            basicFields[i].removeClass("inputField");
                                            basicFields[i].addClass("errorInputField");
                                            submitError=true;                                            
                                    }
                                    else{
                                            basicFields[i].removeClass("errorInputField");
                                            basicFields[i].addClass("inputField");
                                            if(i==6){
                                                remarks=basicFields[i].val();
                                            }
                                            else if(i!=7){
                                                inputData[basicFields[i].attr('id')]=basicFields[i].val();
                                            }
                                    }
                                }                                

                                switch (deviceType){

                                    case '0':
                                    break;

                                    case '1':{
                                        var padFields=new Array();
                                        var namesArray=['sn','make','model','rating','date','supplier','screen','touch','cam','vga','remarks'];
                                        //var itemsArr={};

                                        padFields=[$("#padScreen"),$("#padTouch"),$("#padCam"),$("#padVga")];

                                        inputJsonObj["type"]="AddPad";

                                        for(var i=0;i<padFields.length;i++){

                                            if(padFields[i].val()==""){
                                                padFields[i].removeClass("inputField");
                                                padFields[i].addClass("errorInputField");
                                                submitError=true;
                                            }
                                            else{
                                                    padFields[i].removeClass("errorInputField");
                                                    padFields[i].addClass("inputField");
                                                    //temsArr[padFields[i].attr('id')]=padFields[i].val();
                                                    inputData[padFields[i].attr('id')]=padFields[i].val();
                                            }
                                        }
                                        
                                        inputData['remarks']=remarks;
                                        inputJsonObj["padData"]=inputData;
                                        inputJsonObj['names']=namesArray;

                                        break;
                                    }

                                    case '2':{

                                        var desktopsFields=new Array();
                                        var namesArray=['sn','make','model','rating','date','supplier','cpu','memory','remarks'];

                                        inputJsonObj["type"]="AddDesktop";

                                        desktopsFields=[$("#dtProcessor"),$("#dtMemory")/*,$("#dtVga"),$("#dtHdd")*/];

                                        for(var i=0;i<desktopsFields.length;i++){
                                            if(desktopsFields[i].val()==""){
                                                desktopsFields[i].removeClass("inputField");
                                                desktopsFields[i].addClass("errorInputField");
                                                submitError=true;
                                            }
                                            else{
                                                    desktopsFields[i].removeClass("errorInputField");
                                                    desktopsFields[i].addClass("inputField");
                                                    //itemsArr[desktopsFields[i].attr('id')]=desktopsFields[i].val();
                                                    inputData[desktopsFields[i].attr('id')]=desktopsFields[i].val();
                                            }
                                        }
                                        //inputJsonObj["deskTop"]=itemsArr;
                                        inputData['remarks']=remarks;
                                        //queryType="AddDesktop";

                                        inputJsonObj["desktopData"]=inputData;
                                        inputJsonObj['names']=namesArray;
                                        break;
                                    }


                                    case '3':{
                                        var laptopsFields=new Array();
                                        var namesArray=['sn','make','model','rating','date','supplier','screen','cpu','vga','memory','remarks'];
                                        //var itemsArr={};
                                        inputJsonObj["type"]="AddLaptop";

                                        laptopsFields=[$("#lapScreen"),$("#lapProcessor"),$("#lapVga"),$("#lapMemory")/*,$("#lapHdd")*/];
                                        for(var i=0;i<laptopsFields.length;i++){
                                            if(laptopsFields[i].val()==""){
                                                laptopsFields[i].removeClass("inputField");
                                                laptopsFields[i].addClass("errorInputField");
                                                submitError=true;
                                            }
                                            else{
                                                    laptopsFields[i].removeClass("errorInputField");
                                                    laptopsFields[i].addClass("inputField");
                                                    //itemsArr[laptopsFields[i].attr('id')]=laptopsFields[i].val();
