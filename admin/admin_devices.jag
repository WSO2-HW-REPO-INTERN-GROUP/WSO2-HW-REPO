                                            }
                                        }

                                        //inputJsonObj["laptop"]=itemsArr;
                                        inputData['remarks']=remarks;

                                        //queryType="AddLaptop";

                                        inputJsonObj["AddLcdMonitor"]=inputData;
                                        inputJsonObj['names']=namesArray;

                                        break;
                                    }


                                    case '4':{

                                        var desktopsFields=new Array();
                                        var namesArray=['sn','make','model','rating','date','supplier','cpu','memory','remarks'];

                                        inputJsonObj["type"]="AddServerRoomDesktop";

                                        desktopsFields=[$("#dtProcessor"),$("#dtMemory")/*,$("#dtVga"),$("#dtHdd")*/];

                                        for(var i=0;i<desktopsFields.length;i++){
