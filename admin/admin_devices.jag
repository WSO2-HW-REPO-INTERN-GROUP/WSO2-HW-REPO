            <!-- This script handles the tabbed pane functionality -->
            <script type="text/javascript">
                $(document).ready(function(){                   

                    var basicFields=new Array();
                    var accHealth;
                    basicFields=[$("#serial"),$('#make'),$("#model"),$("#rating"),$("#purchaseDate"),$('#supplier'),$('#remarks'),$("#deviceType"),$("#warrantyEndDate")];
                    var inputData={};
                    var remarks;
                    var dropdownSelect="";                
                    getAllDevices();

                    <!--Add the health bar-->
                    var canvas = document.getElementById('canvas');
                    canvas.width = 150;
                    canvas.height = 50;
                    var context = canvas.getContext('2d');

                    var object1 = {
                        x: 20,
                        y: 30,
                        width: 100,
                        height: 20

                    };

                    var health = 10;
                    var maxHealth = 10;

                    // Loop
                    setInterval(onTimerTick, 33);
                    // Render Loop
                    function onTimerTick() {
                        // Clear the canvas
                        canvas.width = canvas.width;                        
                        // Calculate health bar percent
                        var percent = health / maxHealth;
                        accHealth=health;

                        context.fillStyle = "#9ecaed";
                        context.font = "18px sans-serif";
                        context.fillText("Life " +health+"/"+maxHealth+" = " + percent * 100 +"%", 20, 20);

                        context.fillStyle = "#d9d9d9";
                        context.fillRect(object1.x, object1.y, object1.width, object1.height);

                        context.fillStyle = "#9ecaed";
                        context.fillRect(object1.x, object1.y, object1.width * percent, object1.height);
                    }

                    function contains(target, x, y) {
                        if (x >= target.x && x <= (target.x + target.width) && y >= target.y && y <= (target.y + target.height)) {
                            return true;
                        }
                        return false;
                    }

                    // Update mouse position
                    canvas.onmousemove = function (e) {
                        var mouseX = e.offsetX;
                        var mouseY = e.offsetY;

                        if (contains(object1, mouseX, mouseY)) {
                            health = Math.round(((mouseX - object1.x) / object1.width) * maxHealth);
                        }
                    }

                    <!--Add the health bar-->

                    $("#addNewAccessory").dialog({
                        autoOpen:false,
                        height: 450,
                        width:650,
                        modal: true,
                        buttons:{
                            'Confirm': function() {

                                var accSerial=$("#accSerial").val();
                                var accType=$('#accType').val();
                                var remarks=$("#accRemarks").val();
                                var accPurchaseDate=$("#accPurchase").val();
                                var accWarEndDate=$("#accWarEnd").val();

                                if(accSerial=="" || accType=="0" || remarks=="" || accPurchaseDate=="" || accWarEndDate==""){
                                    document.getElementById("accError").style.display="";
                                }
                                else{
                                    document.getElementById("accError").style.display="none";
                                    var accData={
                                        "serial":accSerial,
                                        "accType":accType,
                                        "remarks":remarks,
                                        "health":accHealth,
                                        "purchaseDate":accPurchaseDate,
                                        "endDate":accWarEndDate
                                    };

                                    $.post(<%print('"'+dataGetFilesLocation+"admin_devices_updates.jag"+'"');%>,{operation:"addAccessory",inputs:JSON.stringify(accData)},function(data,status){
                                        if(data=='SUCCESSFUL'){
                                            location.reload(true);
                                        }
                                        else{
                                            alert("Error While Updating Database");
                                        }
                                    });
                                    $(this).dialog('close');
                                }                             
                            }
                        }
                    });
                
