        <script>
                    jQuery(document).ready(function(){

                        var searchClkCounter=0;
            
                        $( "#fromDatepicker" ).datepicker();
                        $( "#toDatepicker" ).datepicker();
            
                        prepareList();
                        loadAllWarranties();
                        loadIssues();
            
                        <!--Get the selected row's first cell value-->
                        $(document).ready(function(){
                            $("#issuesTablec").delegate('tr > td:first-child', 'click', function() {
                                var issueId = $(this).text();
                                $("#warrantyDetails").append(issueId);
                                alert(issueId);
            
                                $.get("admin_issue_details.jag",
                                        function(data){
                                            window.location=data;
                                        });
            
                            });
                        });
                        <!--*******************-->
            
                        function prepareList() {
                            $('#expList').find('li:has(ul)').unbind('click').click(function(event) {
                                if(this == event.target) {
                                    $(this).toggleClass('expanded');
                                    $(this).children('ul').toggle('medium');
                                }
                                return false;
                            }).addClass('collapsed').removeClass('expanded').children('ul').hide();
            
                            //Hack to add links inside the cv
                            $('#expList a').unbind('click').click(function() {
                                window.open($(this).attr('href'));
                                return false;
                            });
                            //Create the button functionality
                            $('#expandList').unbind('click').click(function() {
                                $('.collapsed').addClass('expanded');
                                $('.collapsed').children().show('medium');
                            })
                            $('#collapseList').unbind('click').click(function() {
                                $('.collapsed').removeClass('expanded');
                                $('.collapsed').children().hide('medium');
                            })
                        }
            
                        function loadAllWarranties(){
            
                            $.post("admin_home_Operations.jag",{operation:"getAllWarranty"},function(data){
                                var warrantyTbl=document.getElementById('allWarrantiesTable');
                                var warrantyArea=document.getElementById("allWarranties");
                                warrantyArea.setAttribute('class','datagrid');
                                var tbody=document.createElement('tbody');
            
                                var objArray=JSON.parse(data);
            
                                for(var i=0;i<objArray.length;i++){

                                        var object=objArray[i];
                                        var newRow=document.createElement('tr');
                                        var warrantyId;

                                        if(i%2==0){
                                            newRow.setAttribute("class","alt");
                                        }

                                        for(var property in object){
                                        var td=document.createElement('td');
                                        td.style.padding='3px 20px';

                                        if(property=="id"){
                                            warrantyId=object[property];
                                        }

                                        td.appendChild(document.createTextNode(object[property]));

                                        newRow.appendChild(td);
                                    }

                                    var linkTd=document.createElement('td');
                                    var link=document.createElement('a');
                                    link.setAttribute("href",("admin_warranty_details.jag?id="+warrantyId));
                                    link.innerHTML='more';
                                    linkTd.appendChild(link);
                                    newRow.appendChild(linkTd);
            
                                    tbody.appendChild(newRow);
                                }
                                warrantyTbl.appendChild(tbody);
                            });
                        }

                        function loadIssues(){
                            $.post("admin_home_Operations.jag",{operation:"getAllissues"},function(data){
                                var warrantyTbl=document.getElementById('issuesTable');
                                var warrantyArea=document.getElementById("allIssuesDiv");
                                warrantyArea.setAttribute('class','datagrid');
                                var tbody=document.createElement('tbody');

                                var objArray=JSON.parse(data);

                                for(var i=0;i<objArray.length;i++){

                                    var issueId;

                                    var object=objArray[i];
                                    var newRow=document.createElement('tr');

                                    if(i%2==0){
                                        newRow.setAttribute("class","alt");
                                    }

                                    for(var property in object){
                                        var td=document.createElement('td');
                                        td.style.padding='3px 20px';

                                        if(property=="id"){
                                            issueId=object[property];
                                        }

                                        td.appendChild(document.createTextNode(object[property]));

                                        newRow.appendChild(td);
                                    }

                                    var linkTd=document.createElement('td');
                                    var link=document.createElement('a');
                                    link.setAttribute("href",("admin_issue_details.jag?id="+issueId));
                                    link.innerHTML='more';
                                    linkTd.appendChild(link);
                                    newRow.appendChild(linkTd);

                                    tbody.appendChild(newRow);
                                }
                                warrantyTbl.appendChild(tbody);
                            });
                        }
            
            
                        $("#warrantySearchButton").click(function(){

                            var searchText=document.getElementById("serialOrId").value;
                            var state=(document.getElementById("warrantyStateSelect"));
                            var filterVal=state.options[state.selectedIndex].value;
                            var errMsgField=document.getElementById("errMessage");

                            if(searchText==""||filterVal==0){
                                errMsgField.classList.remove(errMsgField.className);
                                errMsgField.classList.add('error');
                                errMsgField.textContent="Invalid search Text or Warranty State";
                            }
                            else{
                                errMsgField.classList.remove(errMsgField.className);
                                errMsgField.classList.add('hidden');

                                var allWarrantTable=$("#allWarranties");

                                $.post("admin_requests_handler.jag",{searchText:searchText,filterValue:filterVal,request:"getWarranty"},function(data){

                                    var searchedArea=document.getElementById('allWarranties');
                                    var searchedTable = document.getElementById("allWarrantiesTable");
                                    searchedArea.style.display="";
                                    searchedArea.setAttribute('class','datagrid');
                                    var tbody=document.createElement('tbody');



                                    while ( searchedTable.rows.length > 1 )
                                    {
                                        searchedTable.deleteRow(1);
                                    }


                                    var objArray=JSON.parse(data);

                                    for(var i=0;i<objArray.length;i++){

                                        var object=objArray[i];
                                        var newRow=document.createElement('tr');
                                        var warrantyId;

                                        if(i%2==0){
                                            newRow.setAttribute("class","alt");
                                        }

                                        for(var property in object){
                                        var td=document.createElement('td');
                                        td.style.padding='3px 20px';

                                        if(property=="id"){
                                            warrantyId=object[property];
                                        }

                                        td.appendChild(document.createTextNode(object[property]));

                                        newRow.appendChild(td);
                                    }

                                    var linkTd=document.createElement('td');
                                    var link=document.createElement('a');
                                    link.setAttribute("href",("admin_warranty_details.jag?id="+warrantyId));
                                    link.innerHTML='more';
                                    linkTd.appendChild(link);
                                    newRow.appendChild(linkTd);


                                        tbody.appendChild(newRow);
                                    }

                                    searchedTable.appendChild(tbody);

                                });
                            }
                        });
                    });
                
        </script>
